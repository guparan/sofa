# CMake package configuration file for the SofaLMConstraint plugin

@PACKAGE_INIT@

find_package(SofaFramework REQUIRED)

if(NOT TARGET SofaLMConstraint)
        include("${CMAKE_CURRENT_LIST_DIR}/SofaLMConstraintTargets.cmake")
endif()

set(SOFA_HAVE_SOFALMCONSTRAINT "@SOFA_HAVE_SOFALMCONSTRAINT@")

check_required_components(SofaLMConstraint)
set(SofaLMConstraint_LIBRARIES SOFALMCONSTRAINT)
set(SofaLMConstraint_INCLUDE_DIRS @PACKAGE_SOFALMCONSTRAINT_INCLUDE_DIR@ ${SOFALMCONSTRAINT_INCLUDE_DIR})


